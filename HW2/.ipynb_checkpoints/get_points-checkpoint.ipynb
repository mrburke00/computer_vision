    "import matplotlib.pyplot as plt\n",
    "\n",
    "def get_points(img1, img2):\n",
    "    fig, (ax1, ax2) = plt.subplots(1, 2)\n",
    "    ax1.imshow(img1)\n",
    "    ax2.imshow(img2)\n",
    "\n",
    "    # gets a list of (x,y) or (col, row) coordinates\n",
    "    coord_list = plt.ginput(10, -1, show_clicks=True)\n",
    "\n",
    "    return(coord_list)"
   ]
  }
